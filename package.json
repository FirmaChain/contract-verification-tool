{
    "name": "contract-verification-tool",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "dependencies": {
        "@firmachain/firma-js": "^0.2.62",
        "@mui/icons-material": "^5.8.4",
        "@mui/material": "^5.2.7",
        "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
        "axios": "^1.10.0",
        "date-fns": "^4.1.0",
        "immer": "^10.1.1",
        "notistack": "^2.0.5",
        "react": "^18.3.1",
        "react-device-detect": "^2.1.2",
        "react-dom": "^18.3.1",
        "react-router": "^6.2.1",
        "react-router-dom": "^6.2.1",
        "react-transition-group": "^4.4.5",
        "styled-components": "^5.3.3",
        "zustand": "^5.0.1"
    },
    "devDependencies": {
        "@types/react-dom": "^18.3.1",
        "@types/styled-components": "^5.1.34",
        "@vitejs/plugin-react-swc": "^3.10.2",
        "prettier": "^3.3.3",
        "typescript": "^4.9.5",
        "vite": "^7.0.0",
        "vite-plugin-bundle-obfuscator": "^1.7.0",
        "vite-plugin-node-polyfills": "^0.23.0",
        "vite-tsconfig-paths": "^5.1.4",
        "web-vitals": "^2.1.2"
    },
    "scripts": {
        "dev": "vite dev --mode dev",
        "testnet": "vite dev --mode testnet",
        "mainnet": "vite dev --mode mainnet",
        "build:testnet": "vite build --mode testnet",
        "build:mainnet": "vite build --mode mainnet",
        "deploy-testnet": "aws s3 sync ./build s3://deploy-ecosystem/testnet/verify-tool --profile=firmachain-deploy --delete",
        "invalidate-testnet": "aws cloudfront create-invalidation --profile=firmachain-deploy --distribution-id E2UUAL0XYSVCXP --paths '/*'",
        "deploy-mainnet": "aws s3 sync ./build s3://deploy-ecosystem/mainnet/verify-tool --profile=firmachain-deploy --delete",
        "invalidate-mainnet": "aws cloudfront create-invalidation --profile=firmachain-deploy --distribution-id E12CRKX8WKDZ2A --paths '/*'",
        "format": "prettier --write \"src/**/*.{ts,tsx}\""
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "resolutions": {
        "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
    },
    "packageManager": "yarn@4.9.2"
}
